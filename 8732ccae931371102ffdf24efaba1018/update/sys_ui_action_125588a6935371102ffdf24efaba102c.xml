        <script><![CDATA[function onClick() {
  var gr = new GlideRecord('x_1132934_diligent_efforts');
  gr.addQuery('number', current.sys_id);
  gr.query();
  if (gr.next()) {
    var ci = new CaptureEfforts();
    ci.setParameter('sysparm_punch_in_field', gr.getElement('punch_in'));
    ci.setParameter('sysparm_punch_out_field', gr.getElement('punch_out'));
    if (this.name == 'Punch In') {
      ci.punchIn();
    } else if (this.name == 'Punch Out') {
      ci.punchOut();
    }
  }
}
